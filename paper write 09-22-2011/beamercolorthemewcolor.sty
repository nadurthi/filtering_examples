\mode<presentation>

\setbeamercolor{palette primary}{use=structure,fg=black,bg=structure.fg!40!white}
\setbeamercolor{palette secondary}{use=structure,fg=black,bg=structure.fg!60!white}
\setbeamercolor{palette tertiary}{use=structure,fg=black,bg=structure.fg!70!white}
\setbeamercolor{palette quaternary}{use=structure,fg=black,bg=structure.fg!80!white}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

% inner colors
\setbeamercolor{block title}{use=structure,fg=structure.fg,bg=structure.fg!30!bg}
\setbeamercolor{block title alerted}{use=alerted text,fg=alerted text.fg,bg=alerted text.fg!30!bg}
\setbeamercolor{block title example}{use=example text,fg=example text.fg,bg=example text.fg!30!bg}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!50!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!50!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!50!bg}


\setbeamercolor{palette sidebar primary}{use=structure,fg=black,bg=structure.fg!40!white}
\setbeamercolor{palette sidebar secondary}{use=structure,fg=black,bg=structure.fg!60!white}
\setbeamercolor{palette sidebar tertiary}{use=structure,fg=black,bg=structure.fg!70!white}
\setbeamercolor{palette sidebar quaternary}{use=structure,fg=black,bg=structure.fg!80!white}

\mode
<all>
